<table>
    <tbody>
        <tr>
            <td colspan="3" class="syntax-separator" style="text-align: center; font-weight: bold;">Card data queries</td>
        </tr>
        <tr>
            <td>a</td>
            <td>Attribute</td>
            <td><a href="/search?q=a=dark">a=dark</a>, <a href="/search?q=a:divine">a:divine</a></td>
        </tr>
        <tr>
            <td>c</td>
            <td>Card type</td>
            <td><a href="/search?q=c:monster">c:monster</a>, <a href="/search?q=c:spell">c:spell</a>, <a href="/search?q=c:trap">c:trap</a></td>
        </tr>
        <tr>
            <td>l</td>
            <td>Level / Rank / Link Rating</td>
            <td><a href="/search?q=l=4">l=4</a></td>
        </tr>
        <tr>
            <td>o</td>
            <td>Card Text</td>
            <td><a href="/search?q=o%3A&quot;Special+Summon+this+card&quot;">o:"Special Summon this card"</a></td>
        </tr>
        <tr>
            <td>s</td>
            <td>Pendulum Scale</td>
            <td><a href="/search?q=s=7">s=7</a></td>
        </tr>
        <tr>
            <td>t</td>
            <td>Type line</td>
            <td><a href="/search?q=t:reptile">t:reptile</a>, <a href="/search?q=t:quick-play">t:quick-play</a>, <a href="/search?q=t:tuner">t:tuner</a>, <a href="/search?q=t=synchro">t:synchro</a></td>
        </tr>
        <tr>
            <td>atk</td>
            <td>Attack points</td>
